..//■基A定x
function GalleryBase()
{
	$エキストラ画像=true;

	LoadImage("CgImage0", "cg/sys/extra/cg/空白アイコン.png");
	//LoadImage("CgImage1", "cg/sys/extra/cg/サムネイルボタンoff.png");
	LoadImage("CgImage2", "cg/sys/extra/cg/xkカ`ソル.png");
	//LoadImage("CgImage3", "cg/sys/extra/cg/サムネイルボタンon.png");
	LoadImage("NoData", "cg/sys/extra/cg/未.jpg");

	//■定x「背景」
	CreateTexture("CgGround", 1000, 0, 0, "cg/sys/extra/cg/背景画像.png");

	//■各N定x
	Array($CgNutName,"2D","3D","BG","Back","Next","Exit");
	AssocArray($CgNutName,"2D","3D","BG","Back","Next","Exit");

	Array($CgNutName["2D"],862,98,true,人物画,"2D");
	Array($CgNutName["3D"],862,194,true,三次元的画,"3D");
	Array($CgNutName["BG"],862,334,true,背景画,BG);
	Array($CgNutName["Back"],912,438,true,前,Back);
	Array($CgNutName["Next"],862,438,true,次,Next);
	Array($CgNutName["Exit"],0,438,true,退出,Exit);

	$WhileCount=0;
	while(Count($CgNutName)>$WhileCount){
		if($CgNutName[$WhileCount][2]){
			$CgX=$CgNutName[$WhileCount][0];
			$CgY=$CgNutName[$WhileCount][1];

			$CgNutBase="Cg"+$CgNutName[$WhileCount];
			CreateChoice($CgNutBase);
			SetAlias($CgNutBase, $CgNutBase);

			$CgNutImage1=$CgNutBase+"Image1";
			$CgNutFail="cg/sys/extra/cg/"+$CgNutName[$WhileCount][3]+"-001.png";
			LoadImage($CgNutImage1, $CgNutFail);
			$CgNutImage2=$CgNutBase+"Image2";
			$CgNutFail="cg/sys/extra/cg/"+$CgNutName[$WhileCount][3]+"-002.png";
			LoadImage($CgNutImage2, $CgNutFail);
			$CgNutImage3=$CgNutBase+"Image3";
			$CgNutFail="cg/sys/extra/cg/"+$CgNutName[$WhileCount][3]+"-003.png";
			LoadImage($CgNutImage3, $CgNutFail);

			$CgNutTemp1=$CgNutBase+"/img";
			CreateTexture($CgNutTemp1, 1000, $CgX, $CgY, $CgNutImage1);

			if($CgNutName[$WhileCount]!="Next"&&$CgNutName[$WhileCount]!="Back"&&$CgNutName[$WhileCount]!="Exit"){
				$CgNutTemp2=$CgNutBase+"/img2";
				CreateTexture($CgNutTemp2, 1000, $CgX, $CgY, $CgNutImage3);
			}
			$CgNutHit=$CgNutBase+"/MouseUsual/hit";
			CreateTexture($CgNutHit, 1000, $CgX, $CgY, $CgNutImage1);
			$CgNutOver=$CgNutBase+"/MouseOver/nut";
			CreateTexture($CgNutOver, 1000, $CgX, $CgY, $CgNutImage2);
			$CgNutClick=$CgNutBase+"/MouseClick/nut";
			CreateTexture($CgNutClick, 1000, $CgX, $CgY, $CgNutImage3);
			Request($CgNutHit, "Erase");
		}
		$WhileCount++;
	}

	//■ＣＧ涫定x
	if(!$PLACE_title){
		Cg();
	}

	//ペ`ジ合き出し
	$CgWrite=false;
	CgWrite();
}


