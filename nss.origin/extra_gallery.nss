{
	$CgFadeBreak=false;

	$CgNumber=$SelectCgIco+(($CgPage-1)*$CgMaxCount);
	$CgName=#CgCategory[$CgPhase][$CgNumber];

	$CountWhile=#Cg要素数;
	while(Count(#Cg[$CgName])>$CountWhile){

		//\I理
		if($CountWhile>#Cg要素数){
			$PreCountWhile=$CountWhile-1;
			if(VariableValue(#,#Cg[$CgName][$PreCountWhile])==false&&VariableValue(#,#Cg[$CgName][$CountWhile])==false){
				CreateColor("CgBlack", 3000, 0, 0, 1024, 576, "BLACK");
				Fade("CgBlack", 0, 0, null, true);
				Fade("CgBlack", 150, 1000, null, true);
			}
		}

		if(VariableValue(#,#Cg[$CgName][$CountWhile])){
			//文字列判定
			if(Strstr(#Cg[$CgName][$CountWhile], "ev")){
				$index="ev";
			}else{
				$index="bg";
			}
			//パッチ用
			$PatchA=#Cg[$CgName][$CountWhile];
			if($PatchA=="ev136_殛L解hＶＳ首I３_d"||$PatchA=="ev163_真っ二つの正宗_c"||$PatchA=="ev221_月明かりを浴びて立つ光_a"||$PatchA=="ev221_月明かりを浴びて立つ光_b"||$PatchA=="ev222_茶々丸Ｈ_c"||$PatchA=="ev222_茶々丸Ｈ_d"||$PatchA=="bg031_八幡m境内_03"){
				$head="cg2/";
			}else{
				$head="cg/";
			}
			$img=$head+$index+"/"+#Cg[$CgName][$CountWhile]+".jpg";
		}else{
			$img="NoData";
		}
		$nut="CgTex"+$CountWhile;
		CreateTexture($nut, 3000, 0, 0, $img);
		Fade($nut, 0, 0, null, true);

		//L物位置{整
		$nutH=ImageHorizon($nut);
		$nutV=ImageVertical($nut);
		if($nutV>576){
			$BaseNutV=$nutV-576;
			$MovePointV=-($BaseNutV);
			if(#Cg[$CgName][1]=="Vr"){
				Move($nut, 0, @0, $MovePointV, null, true);
				$MovePointV=0;
			}
				$MoveTime=$BaseNutV*12;
				$MovePointH=0;
		}else if($nutH>1024){
			$BaseNutH=$nutH-1024;
			$MovePointH=-($BaseNutH);
			if(#Cg[$CgName][1]=="Hr"){
				Move($nut, 0, @0, $MovePointH, null, true);
				$MovePointH=0;
			}
				$MoveTime=$BaseNutH*12;
				$MovePointV=0;
		}

		//通常描画
		Fade($nut, 500, 1000, null, false);
		WaitKey(500);
		Fade($prenut, 0, 0, null, false);
		Fade($nut, 0, 1000, null, true);
		Delete($prenut);
		CgCansel();
		select{
			if($SYSTEM_r_button_down||$SYSTEM_keydown_ctrl||$SYSTEM_keydown_space){$CgFadeBreak=true;break;}
			else if($SYSTEM_l_button_down||$SYSTEM_keydown_enter){break;}
		}
		if($CgFadeBreak){break;}

		//L物描画
		if($nutV>576||$nutH>1024){
			Move($nut, $MoveTime, $MovePointH, $MovePointV, null, false);

			$cgwait=false;
			CgCansel();
			select{
				Position($nut,$nutX,$nutY);
				if(($MovePointH==$nutX)&&($MovePointV==$nutY)){
					if($SYSTEM_r_button_down||$SYSTEM_keydown_ctrl||$SYSTEM_keydown_space){
						$CgFadeBreak=true;break;
					}else if($SYSTEM_l_button_down||$SYSTEM_keydown_enter){
						break;
					}
				}else{
					if($SYSTEM_r_button_down||$SYSTEM_keydown_ctrl||$SYSTEM_keydown_space){
						Move($nut, 0, $MovePointH, $MovePointV, null, true);
						CgCansel();
					}else if($SYSTEM_l_button_down||$SYSTEM_keydown_enter){
						CgCansel();
						if($cgwait){
							$nutmx=$MovePointH-$nutX;
							$nutmy=$MovePointV-$nutY;
							if($nutmx<0){$nutmx=-$nutmx;}
							if($nutmy<0){$nutmy=-$nutmy;}
							if($nutmx!=0){
								$MoveTime=($BaseNutH*12)*($nutmx/$BaseNutH);
							}else if($nutmy!=0){
								$MoveTime=($BaseNutV*12)*($nutmy/$BaseNutV);
							}
							Move($nut, $MoveTime, $MovePointH, $MovePointV, null, false);
							$cgwait=false;
						}else{
							Move($nut, 0, @0, @0, null, true);
							$cgwait=true;
						}
					}
				}
			}
		}
		if($CgFadeBreak){break;}

		$prenut=$nut;
		$CountWhile++;
	}
	Delete("CgBlack");

	Fade($nut, 300, 0, null, true);
	Delete($nut);
	$prenut=0;

	$SelectCgIco=0;

	CgCansel();
}

function CgCansel(){
	$SYSTEM_keydown_enter=false;
	$SYSTEM_keydown_ctrl=false;
	$SYSTEM_l_button_down=false;
	$SYSTEM_r_button_down=false;
	$SYSTEM_keydown_space=false;
}


